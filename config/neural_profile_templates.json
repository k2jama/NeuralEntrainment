    "custom_profile_builder": {
        "assessment_questions": {
            "sensitivity_assessment": [
                "How do you typically respond to loud noises or bright lights?",
                "Do you consider yourself highly sensitive to environmental stimuli?",
                "How do you react to changes in your environment?",
                "Do you pick up on subtle emotional cues from others easily?"
            ],
            "experience_assessment": [
                "How much experience do you have with meditation or consciousness practices?",
                "Have you used neural entrainment or binaural beats before?",
                "How comfortable are you with altered states of consciousness?",
                "What is your familiarity with personal development practices?"
            ],
            "current_state_assessment": [
                "How would you describe your current mental/emotional state?",
                "Are you currently experiencing any stress or anxiety?",
                "How is your overall energy level today?",
                "Do you have any current concerns or worries?"
            ],
            "intention_assessment": [
                "What do you hope to achieve with this session?",
                "Are you seeking relaxation, focus, creativity, or healing?",
                "How important is it to you to go deep vs. staying comfortable?",
                "What would success look like for you?"
            ]
        },
        "scoring_guidelines": {
            "sensitivity_scoring": {
                "high_sensitivity_indicators": ["environmental_sensitivity", "emotional_sensitivity", "noise_sensitivity"],
                "low_sensitivity_indicators": ["stress_tolerance", "stimulation_seeking", "environmental_resilience"]
            },
            "experience_scoring": {
                "beginner_indicators": ["no_meditation", "new_to_altered_states", "limited_practice"],
                "advanced_indicators": ["regular_meditation", "consciousness_exploration", "teaching_experience"]
            }
        }
    },

